const celsiusToFahrenheit = temp => {
	const fahrenheit = (temp * 9)/5+32;
	console.log(`Temperature in Celsius ${temp} \nTemperature in Fahrenheit ${fahrenheit}`)
}

//testing
celsiusToFahrenheit(1);
//result
-> Temperature in Calsius 1 
-> Temperature in Fahrenheit 33.8


const fahrenheitToCelsius = temp =>{
    const celsius = (temp - 32) * 5/9;
    console.log(`Temperature in Fahrenheit ${temp} \nTemperature in Celsius ${Math.ceil(celsius)}`)
}
//testing
fahrenheitToCelsius(33.8)
-> Temperature in Fahrenheit 33.8 
-> Temperature in Celsius 1
